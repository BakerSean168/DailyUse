{
  "name": "sync-client",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/sync-client",
  "projectType": "library",
  "tags": ["scope:shared", "type:api-client"],
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/sync-client",
        "main": "packages/sync-client/index.ts",
        "tsConfig": "packages/sync-client/tsconfig.json",
        "assets": ["packages/sync-client/README.md"]
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "options": {
        "lintFilePatterns": ["packages/sync-client/**/*.ts"]
      }
    },
    "regenerate": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npx @openapitools/openapi-generator-cli generate -i docs/api/sync-api.yaml -g typescript-axios -o packages/sync-client --additional-properties=supportsES6=true,npmName=sync-client,npmVersion=1.0.0"
      }
    }
  }
}
