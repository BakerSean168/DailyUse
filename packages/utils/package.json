  },
  "dependencies": {
    "date-fns": "^4.1.0",
    "uuid": "^11.0.5"
  },
  "devDependencies": {
    "tsup": "^8.3.5",
    "typescript": "^5.8.3",
    "rimraf": "^6.0.1"
  }
}
