{
  "name": "@dailyuse/utils",
  "version": "0.0.1",
  "private": false,
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "clean": "node ../../node_modules/rimraf/dist/esm/bin.mjs dist",
    "typecheck": "node ../../node_modules/typescript/lib/tsc.js --build tsconfig.json",
    "build": "pnpm typecheck && node ../../node_modules/tsup/dist/cli-default.js",
    "dev": "node ../../node_modules/concurrently/dist/bin/concurrently.js \"pnpm typecheck --watch\" \"node ../../node_modules/tsup/dist/cli-default.js --watch\""
  },
  "dependencies": {
    "date-fns": "^4.1.0",
    "mitt": "^3.0.1",
    "uuid": "^11.0.5"
  },
  "devDependencies": {
    "@types/node": "^24.3.1",
    "rimraf": "^6.0.1",
    "tsup": "^8.3.5",
    "typescript": "^5.8.3",
    "concurrently": "^8.2.0"
  }
}
