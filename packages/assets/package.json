{
  "name": "@dailyuse/assets",
  "version": "0.0.1",
  "description": "Shared static assets for DailyUse applications (images, audio, fonts)",
  "private": false,
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./images": {
      "types": "./dist/images/index.d.ts",
      "import": "./dist/images/index.js"
    },
    "./audio": {
      "types": "./dist/audio/index.d.ts",
      "import": "./dist/audio/index.js"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "build": "vite build",
    "dev": "vite build --watch"
  },
  "devDependencies": {
    "rimraf": "^6.0.1",
    "typescript": "^5.8.3",
    "vite": "^5.1.6",
    "vite-plugin-dts": "^4.5.0"
  },
  "keywords": [
    "assets",
    "static-resources",
    "monorepo"
  ],
  "author": "BakerSean168",
  "license": "MIT"
}
