{
  "name": "@dailyuse/domain-server",
  "version": "0.0.1",
  "description": "Server-side domain models with business logic",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./task": {
      "types": "./dist/task/index.d.ts",
      "import": "./dist/task/index.js"
    },
    "./goal": {
      "types": "./dist/goal/index.d.ts",
      "import": "./dist/goal/index.js"
    },
    "./reminder": {
      "types": "./dist/reminder/index.d.ts",
      "import": "./dist/reminder/index.js"
    },
    "./schedule": {
      "types": "./dist/schedule/index.d.ts",
      "import": "./dist/schedule/index.js"
    },
    "./notification": {
      "types": "./dist/notification/index.d.ts",
      "import": "./dist/notification/index.js"
    },
    "./repository": {
      "types": "./dist/repository/index.d.ts",
      "import": "./dist/repository/index.js"
    },
    "./account": {
      "types": "./dist/account/index.d.ts",
      "import": "./dist/account/index.js"
    },
    "./authentication": {
      "types": "./dist/authentication/index.d.ts",
      "import": "./dist/authentication/index.js"
    },
    "./setting": {
      "types": "./dist/setting/index.d.ts",
      "import": "./dist/setting/index.js"
    },
    "./dashboard": {
      "types": "./dist/dashboard/index.d.ts",
      "import": "./dist/dashboard/index.js"
    },
    "./editor": {
      "types": "./dist/editor/index.d.ts",
      "import": "./dist/editor/index.js"
    },
    "./ai": {
      "types": "./dist/ai/index.d.ts",
      "import": "./dist/ai/index.js"
    }
  },
  "scripts": {
    "typecheck": "node ../../node_modules/typescript/lib/tsc.js --build",
    "build": "pnpm typecheck && node ../../node_modules/tsup/dist/cli-default.js",
    "dev": "node ../../node_modules/concurrently/dist/bin/concurrently.js \"pnpm typecheck --watch\" \"node ../../node_modules/tsup/dist/cli-default.js --watch\"",
    "test": "node ../../node_modules/vitest/vitest.mjs",
    "test:run": "node ../../node_modules/vitest/vitest.mjs run",
    "test:coverage": "node ../../node_modules/vitest/vitest.mjs run --coverage",
    "test:watch": "node ../../node_modules/vitest/vitest.mjs watch",
    "test:ui": "node ../../node_modules/vitest/vitest.mjs --ui"
  },
  "dependencies": {
    "@dailyuse/utils": "workspace:*",
    "@dailyuse/contracts": "workspace:*",
    "bcryptjs": "^2.4.3",
    "date-fns": "^4.1.0",
    "jsonwebtoken": "^9.0.2",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@ai-sdk/openai": "2.0.64",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.6",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "ai": "5.0.89",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0",
    "vitest": "^3.2.4",
    "zod": "4.1.12"
  }
}
