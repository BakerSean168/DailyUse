{
  "name": "@dailyuse/ui-vue",
  "version": "0.0.1",
  "description": "Vue 3 composables wrapping @dailyuse/ui-core headless logic",
  "private": false,
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "build": "node ../../node_modules/tsup/dist/cli-default.js",
    "dev": "node ../../node_modules/tsup/dist/cli-default.js --watch",
    "type-check": "node ../../node_modules/typescript/lib/tsc.js --noEmit"
  },
  "dependencies": {
    "@dailyuse/ui-core": "workspace:*"
  },
  "peerDependencies": {
    "vue": "^3.4.0"
  },
  "devDependencies": {
    "rimraf": "^6.0.1",
    "tsup": "^8.0.0",
    "typescript": "^5.9.3",
    "vue": "^3.4.0"
  }
}
