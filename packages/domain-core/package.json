{
  "name": "@dailyuse/domain-core",
  "version": "0.0.1",
  "description": "Core domain models shared between frontend and backend",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "module": "dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --dts --format esm --target es2020 --out-dir dist",
    "dev": "tsup src/index.ts --dts --format esm --target es2020 --watch --out-dir dist"
  },
  "dependencies": {
    "@dailyuse/contracts": "workspace:*",
    "@dailyuse/utils": "workspace:*",
    "date-fns": "^4.1.0"
  },
  "devDependencies": {
    "tsup": "^8.5.0",
    "typescript": "^5.0.0"
  }
}
