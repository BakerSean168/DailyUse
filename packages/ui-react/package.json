{
  "name": "@dailyuse/ui-react",
  "version": "0.0.1",
  "description": "React hooks wrapping @dailyuse/ui-core headless logic",
  "private": false,
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "build": "node ../../node_modules/tsup/dist/cli-default.js",
    "dev": "node ../../node_modules/tsup/dist/cli-default.js --watch",
    "type-check": "node ../../node_modules/typescript/lib/tsc.js --noEmit"
  },
  "dependencies": {
    "@dailyuse/ui-core": "workspace:*"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0"
  },
  "devDependencies": {
    "@types/react": "^18.3.0",
    "react": "^18.3.0",
    "rimraf": "^6.0.1",
    "tsup": "^8.0.0",
    "typescript": "^5.8.3"
  }
}
