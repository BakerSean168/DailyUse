{
  "name": "@dailyuse/contracts",
  "version": "0.0.1",
  "private": false,
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "clean": "rimraf dist",
    "build": "tsup src/index.ts --dts --format esm --target es2020 --out-dir dist",
    "dev": "tsup src/index.ts --watch --dts --format esm --target es2020 --out-dir dist"
  },
  "dependencies": {
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "tsup": "^8.3.5",
    "typescript": "^5.8.3",
    "rimraf": "^6.0.1"
  }
}
