{
  "name": "@dailyuse/contracts",
  "version": "0.0.1",
  "private": false,
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "development": {
        "types": "./src/index.ts",
        "import": "./src/index.ts"
      },
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "clean": "node ../../node_modules/rimraf/dist/esm/bin.mjs dist",
    "typecheck": "node ../../node_modules/typescript/lib/tsc.js --noEmit",
    "build": "node ../../node_modules/tsup/dist/cli-default.js",
    "dev": "node ../../node_modules/concurrently/dist/bin/concurrently.js \"pnpm typecheck --watch\" \"node ../../node_modules/tsup/dist/cli-default.js --watch\""
  },
  "dependencies": {
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "tsup": "^8.3.5",
    "typescript": "^5.8.3",
    "rimraf": "^6.0.1",
    "concurrently": "^8.2.0"
  },
  "publishConfig": {
    "exports": {
      ".": {
        "types": "./dist/index.d.ts",
        "import": "./dist/index.js"
      }
    }
  }
}
