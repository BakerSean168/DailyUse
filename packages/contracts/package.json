{
  "name": "@dailyuse/contracts",
  "version": "0.0.1",
  "private": false,
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "development": {
        "types": "./src/index.ts",
        "import": "./src/index.ts"
      },
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "clean": "rimraf dist",
    "typecheck": "tsc --build",
    "build": "pnpm typecheck && tsup",
    "dev": "concurrently \"pnpm typecheck --watch\" \"tsup --watch\""
  },
  "dependencies": {
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "tsup": "^8.3.5",
    "typescript": "^5.8.3",
    "rimraf": "^6.0.1",
    "concurrently": "^8.2.0"
  },
  "publishConfig": {
    "exports": {
      ".": {
        "types": "./dist/index.d.ts",
        "import": "./dist/index.js"
      }
    }
  }
}
