{
  "name": "ui-vuetify",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "library",
  "sourceRoot": "packages/ui-vuetify/src",
  "tags": ["scope:ui", "type:vuetify"],
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "outputs": ["{workspaceRoot}/packages/ui-vuetify/dist"],
      "dependsOn": ["^build"],
      "options": {
        "command": "vite build",
        "cwd": "packages/ui-vuetify"
      }
    },
    "serve": {
      "executor": "nx:run-commands",
      "options": {
        "command": "vite build --watch",
        "cwd": "packages/ui-vuetify"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/ui-vuetify/**/*.{ts,js,vue}"]
      }
    }
  },
  "implicitDependencies": ["ui-vue", "contracts"]
}
