{
  "name": "@dailyuse/domain-client",
  "version": "0.0.1",
  "description": "Client-side domain models with UI-focused methods",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "typecheck": "tsc --build",
    "build": "pnpm typecheck && tsup",
    "dev": "concurrently \"pnpm typecheck --watch\" \"tsup --watch\"",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest watch",
    "test:ui": "vitest --ui"
  },
  "dependencies": {
    "@dailyuse/domain-core": "workspace:*",
    "@dailyuse/utils": "workspace:*"
  },
  "devDependencies": {
    "typescript": "^5.0.0",
    "tsup": "^8.0.0",
    "vitest": "^3.2.4",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "happy-dom": "^18.0.1"
  }
}
