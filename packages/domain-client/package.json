{
  "name": "@dailyuse/domain-client",
  "version": "0.0.1",
  "description": "Client-side domain models with UI-focused methods",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./task": {
      "types": "./dist/task/index.d.ts",
      "import": "./dist/task/index.js",
      "require": "./dist/task/index.js"
    },
    "./goal": {
      "types": "./dist/goal/index.d.ts",
      "import": "./dist/goal/index.js",
      "require": "./dist/goal/index.js"
    },
    "./reminder": {
      "types": "./dist/reminder/index.d.ts",
      "import": "./dist/reminder/index.js",
      "require": "./dist/reminder/index.js"
    },
    "./schedule": {
      "types": "./dist/schedule/index.d.ts",
      "import": "./dist/schedule/index.js",
      "require": "./dist/schedule/index.js"
    },
    "./notification": {
      "types": "./dist/notification/index.d.ts",
      "import": "./dist/notification/index.js",
      "require": "./dist/notification/index.js"
    },
    "./repository": {
      "types": "./dist/repository/index.d.ts",
      "import": "./dist/repository/index.js",
      "require": "./dist/repository/index.js"
    },
    "./account": {
      "types": "./dist/account/index.d.ts",
      "import": "./dist/account/index.js",
      "require": "./dist/account/index.js"
    },
    "./authentication": {
      "types": "./dist/authentication/index.d.ts",
      "import": "./dist/authentication/index.js",
      "require": "./dist/authentication/index.js"
    },
    "./setting": {
      "types": "./dist/setting/index.d.ts",
      "import": "./dist/setting/index.js",
      "require": "./dist/setting/index.js"
    },
    "./dashboard": {
      "types": "./dist/dashboard/index.d.ts",
      "import": "./dist/dashboard/index.js",
      "require": "./dist/dashboard/index.js"
    },
    "./editor": {
      "types": "./dist/editor/index.d.ts",
      "import": "./dist/editor/index.js",
      "require": "./dist/editor/index.js"
    },
    "./ai": {
      "types": "./dist/ai/index.d.ts",
      "import": "./dist/ai/index.js",
      "require": "./dist/ai/index.js"
    }
  },
  "scripts": {
    "typecheck": "node ../../node_modules/typescript/lib/tsc.js --noEmit",
    "build": "node ../../node_modules/tsup/dist/cli-default.js",
    "dev": "node ../../node_modules/concurrently/dist/bin/concurrently.js \"pnpm typecheck --watch\" \"node ../../node_modules/tsup/dist/cli-default.js --watch\"",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest watch",
    "test:ui": "vitest --ui"
  },
  "dependencies": {
    "@dailyuse/utils": "workspace:*",
    "date-fns": "^4.1.0"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "^4.0.15",
    "@vitest/ui": "^4.0.15",
    "happy-dom": "^18.0.1",
    "tsup": "^8.0.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.15"
  }
}
